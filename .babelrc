{
  "presets": [
    ["env", {
      "targets": {
        "chrome": 56
      },
      "useBuiltIns": true
    }]
  ],
  "plugins": [
    "lodash"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "presets": ["babili"],
      "plugins": [
        "transform-remove-debugger",
        "transform-remove-console",
        "minify-mangle-names"
      ]
    }
  }
}