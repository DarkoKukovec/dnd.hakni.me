{
  "name": "dnd-hakni-me",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "deploy": "yarn build && now -c ./config/now.json dist/",
    "start": "npm run dev",
    "dev": "NODE_ENV=development nodemon -i node_modules -x webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "storybook": "NODE_ENV=development start-storybook --port 9009",
    "test": "NODE_ENV=test NODE_PATH=./app nyc mocha",
    "lint": "tslint './app/**/*.tsx' './app/**/*.ts'",
    "watch": "nodemon -e .ts -i node_modules -i dist -i coverage -x mocha",
    "precommit": "npm run lint && npm test"
  },
  "author": "Darko <darko@kukovec.eu>",
  "license": "MIT",
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "app/interfaces/*",
      "app/**/*.d.ts",
      "app/**/*.test.ts",
      "app/**/*.test.tsx",
      "app/**/*.story.tsx",
      "test/*",
      "typings/*",
      "node_modules",
      "dist",
      "coverage",
      ".awcache",
      ".nyc_output",
      ".storybook"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false,
    "all": true,
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "dependencies": {
    "@kadira/storybook": "^2.35.3",
    "@types/chai": "^3.4.34",
    "@types/chai-enzyme": "^0.5.7",
    "@types/enzyme": "^2.7.4",
    "@types/lodash": "^4.14.52",
    "@types/loglevel": "^1.4.29",
    "@types/mocha": "^2.2.39",
    "@types/nock": "^8.2.0",
    "@types/node": "^7.0.5",
    "@types/react": "^15.0.9",
    "@types/react-dom": "^0.14.23",
    "@types/react-router": "^3.0.3",
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babili": "^0.0.11",
    "babili-webpack-plugin": "^0.0.10",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "enzyme": "^2.7.1",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "json-loader": "^0.5.4",
    "localstorage-memory": "^1.0.2",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.0",
    "loglevel": "^1.4.1",
    "mobx": "^3.1.0",
    "mobx-collection-store": "^0.8.0",
    "mobx-keys-store": "^1.0.0",
    "mobx-react": "^4.1.0",
    "mocha": "^3.2.0",
    "nock": "^9.0.5",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "normalize.css": "^5.0.0",
    "now": "^4.3.0",
    "nyc": "^10.1.2",
