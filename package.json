{
  "name": "dnd-hakni-me",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "deploy": "yarn build && scp -r dist/. ubuntu@dnd.hakni.me:/www/dnd.hakni.me/",
    "start": "npm run dev",
    "dev": "NODE_ENV=development nodemon -i node_modules -x webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "storybook": "NODE_ENV=development start-storybook --port 9009",
    "test_old": "NODE_ENV=test NODE_PATH=./app nyc mocha",
    "test": "NODE_ENV=test webpack --config config/webpack.test.js && mocha",
    "lint": "tslint './app/**/*.tsx' './app/**/*.ts'",
    "watch": "nodemon -e .ts -i node_modules -i dist -i coverage -x mocha",
    "precommit": "npm run lint && npm test"
  },
  "author": "Darko <darko@kukovec.eu>",
  "license": "MIT",
  "nyc": {
    "sourceMap": true,
    "instrument": true,
    "cache": false,
    "exclude": [
      "app/interfaces/*",
      "app/**/*.d.ts",
      "app/**/*.test.ts",
      "app/**/*.test.tsx",
      "app/**/*.story.tsx",
      "test/*",
      "typings/*",
      "node_modules",
      "dist",
      "coverage",
      ".awcache",
      ".nyc_output",
      ".storybook",
      "~/*",
      "webpack"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "now": {
    "alias": "dnd.hakni.me"
  },
