{
  "name": "typescript-react-mobx-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "deploy": "yarn build && now --alias dnd.hakni.me dist/",
    "start": "npm run dev",
    "dev": "NODE_ENV=development nodemon -i node_modules -x webpack-dev-server",
    "build": "NODE_ENV=production webpack",
    "storybook": "NODE_ENV=development start-storybook --port 9009",
    "test": "NODE_ENV=test nyc mocha",
    "lint": "tslint './app/**/*.tsx' './app/**/*.ts'",
    "watch": "nodemon -e .ts -i node_modules -i dist -i coverage -x mocha",
    "precommit": "npm run lint && npm test"
  },
  "author": "Darko <darko@kukovec.eu>",
  "license": "MIT",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "app/interfaces/*",
      "app/**/*.d.ts",
      "app/**/*.test.ts",
      "app/**/*.test.tsx",
      "app/**/*.story.tsx",
      "test/*",
      "typings/*"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false,
    "all": true,
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "dependencies": {
    "@kadira/storybook": "^2.35.3",
    "@types/chai": "^3.4.34",
    "@types/lodash": "^4.14.50",
    "@types/loglevel": "^1.4.29",
    "@types/mocha": "^2.2.38",
    "@types/nock": "^8.2.0",
    "@types/node": "^7.0.4",
    "@types/react": "^15.0.4",
    "@types/react-dom": "^0.14.21",
    "@types/react-router": "^3.0.0",
    "autoprefixer": "^6.7.0",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-babili": "^0.0.10",
    "babel-preset-env": "^1.1.8",
    "babili-webpack-plugin": "^0.0.9",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "enzyme": "^2.7.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.0",
    "loglevel": "^1.4.1",
    "mobx": "^3.0.2",
    "mobx-collection-store": "^0.8.0",
    "mobx-keys-store": "^1.0.0",
    "mobx-react": "^4.1.0",
    "mocha": "^3.2.0",
    "nock": "^9.0.2",
    "node-sass": "^4.4.0",
    "nodemon": "^1.11.0",
    "normalize.css": "^5.0.0",
    "now": "^4.0.0",
    "nyc": "^10.1.2",
    "postcss-loader": "^1.2.2",
    "react": "^15.4.2",
    "react-css-themr": "^1.7.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-toolbox": "1",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "ts-node": "^2.0.0",
    "tslint": "^4.4.2",
    "typescript": "^2.1.5",
    "typings": "^2.1.0",
    "webpack": "beta",
    "webpack-dev-server": "beta",
    "whatwg-fetch": "^2.0.2"
  }
}
